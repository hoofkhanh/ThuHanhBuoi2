
void Insertion_Sort_tangDan(int a[],int n){
	for(int i=0;i<n;i++){
		int x=a[i],j=i-1;
		while(j>=0 && a[j]>x){
			a[j+1]=a[j];
			j--;
		}
		a[++j]=x;
	}
}

void Insertion_Sort_giamDan(int a[],int n){
for(int i=0;i<n;i++){
		int x=a[i],j=i-1;
		while(j>=0 && a[j]<x){
			a[j+1]=a[j];
			j--;
		}
		a[++j]=x;
	}
}
